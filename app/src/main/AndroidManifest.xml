<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.namget.customline">

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW"></uses-permission>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".SelectActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".main.MainActivity">
        </activity>
        <activity android:name=".seekbar.SeekbarActivity">
        </activity>
        <activity android:name=".gradation.GradationActivity">
        </activity>
        <activity android:name=".line.LineActivity">
        </activity>
        <activity android:name=".scroll.ScrollActivity">
        </activity>
        <activity android:name=".circleview.CircleActivity">
        </activity>

        <activity android:name=".bgfloating.FloatingActivity"
                  android:launchMode="singleTop">
        </activity>

        <activity android:name=".recyclertest.RecyclerActivity">
        </activity>

        <service
                android:name=".bgfloating.FloatingService"
                android:enabled="true"
                android:permission="android.permission.SYSTEM_ALERT_WINDOW">
        </service>

    </application>

</manifest>